
# 超级鹰验证码破解账号
[chaojiying]
accounts_json_path = "保存账号信息的json文件路径"


# MySQL数据库配置
[mysql]
host = ""
port = 3306
user = ""
passwd = ""


# app 成信钟楼相关配置
[weiboclock]
timezone = "Asia/Shanghai"  # 定时任务时区配置
webapi_addr = ":1733"  # http服务运行地址
basic_auth_username = ""  # http服务请求时需要的基础认证账号用户名
basic_auth_passwd = ""  # http服务请求时需要的基础认证账号密码
pic_path = "default"  # 指定 pic_path 时使用该指定的目录中的png图片（图片以整点数为名）,图片不能大于5M, 为空则不发送图片,default使用内置图片
# 微博账号信息相关配置
app_key = "你的appkey"
app_secret = "你的appsecret"
redirect_uri = "你的回调uri"
username = "微博登录账号"
passwd = "微博密码"
security_url = "http://+你设置的安全域名"

# 如果配置了测试账号，则会使用测试账号发微博
test_username = "微博测试账号"
test_passwd = "测试账号密码"

# wttrin参数配置
wttrin_lang = "天气预报语言"
wttrin_location = "天气预报的地点"
wttrin_refresh_schedule = "0 58 * * * *"  # 在整点前2分钟更新天气，整点的wttrin服务不稳定


# app reminder相关配置 （大致同上）
[reminder]
timezone = "Asia/Shanghai"
webapi_addr = ":2109"
basic_auth_username = ""
basic_auth_passwd = ""
# 微博账号信息相关配置
app_key = ""
app_secret = ""
redirect_uri = ""
username = ""
passwd = ""
security_url = ""
wttrin_lang = ""
wttrin_location = ""
wttrin_refresh_schedule = ""

test_username = ""
test_passwd = ""

remind_list = ""  # 被提醒人微博昵称 多个空格分隔
